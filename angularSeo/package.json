{
    "name": "angularSeo",
    "version": "0.0.0",
    "scripts": {
        "ng": "ng",
        "start": "ng serve",
        "build": "ng build",
        "test": "ng test",
        "lint": "ng lint",
        "e2e": "ng e2e",
        "dev:ssr": "ng run angularSeo:serve-ssr",
        "serve:ssr": "node dist/angularSeo/server/main.js",
        "build:ssr": "ng build --prod && ng run angularSeo:server:production",
        "prerender": "ng run angularSeo:prerender",
        "serve:sls": "serverless offline start",
        "build:sls": "ng build --prod --base-href /dev/ && ng run angularSeo:serverless:production",
        "deploy": "serverless deploy",
        "quick:localdeploy": "ng build --prod && ng run angularSeo:serverless:production && serverless offline start",
        "quick:deploy": "ng build --prod && ng run angularSeo:serverless:production && serverless deploy",
        "terminate": "serverless remove"
    },
    "private": true,
    "dependencies": {
        "@angular/animations": "~10.0.0",
        "@angular/common": "~10.0.0",
        "@angular/compiler": "~10.0.0",
        "@angular/core": "~10.0.0",
        "@angular/forms": "~10.0.0",
        "@angular/platform-browser": "~10.0.0",
        "@angular/platform-browser-dynamic": "~10.0.0",
        "@angular/platform-server": "~10.0.0",
        "@angular/router": "~10.0.0",
        "@nguniversal/express-engine": "^10.0.1",
        "aws-serverless-express": "^3.3.8",
        "cors": "^2.8.5",
        "express": "^4.15.2",
        "rxjs": "~6.5.5",
        "serverless": "^1.76.1",
        "serverless-apigw-binary": "^0.4.4",
        "serverless-offline": "^6.5.0",
        "tslib": "^2.0.0",
        "zone.js": "~0.10.3"
    },
    "devDependencies": {
        "@angular-devkit/build-angular": "^0.1100.2",
        "@angular/cli": "~10.0.0",
        "@angular/compiler-cli": "~10.0.0",
        "@nguniversal/builders": "^10.0.1",
        "@types/express": "^4.17.0",
        "@types/jasmine": "~3.5.0",
        "@types/jasminewd2": "~2.0.3",
        "@types/node": "^12.11.1",
        "codelyzer": "^6.0.0-next.1",
        "jasmine-core": "~3.5.0",
        "jasmine-spec-reporter": "~5.0.0",
        "karma": "~5.0.0",
        "karma-chrome-launcher": "~3.1.0",
        "karma-coverage-istanbul-reporter": "~3.0.2",
        "karma-jasmine": "~3.3.0",
        "karma-jasmine-html-reporter": "^1.5.0",
        "protractor": "~7.0.0",
        "ts-node": "~8.3.0",
        "tslint": "~6.1.0",
        "typescript": "~3.9.5"
    }
}